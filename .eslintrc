{
	"root": true,
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": [
		"plugin:vue/vue3-essential",
		"eslint:recommended",
		"@vue/prettier",
		"@vue/eslint-config-prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"rules": {
		"prettier/prettier": ["error"],
		"vue/no-unused-components": "off",
		"no-unused-vars": "off"
	},
	"globals": {
		"_": true
	}
}
